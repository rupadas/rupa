/* Tooltip Starts */
.tooltip {
	position: relative;
	display: inline-block;
	opacity: 1;
}
.tooltip-title { font-weight: bold; }
  
.tooltip .tooltiptext {
	visibility: hidden;
	min-width: 300px;
	background-color: #00A59B;
	color: #fff;
	border-radius: 2px;
	padding: 25px 0 25px 15px;
	position: absolute;
	z-index: 1;
	top: 130%;
	left: 1%;
	white-space: normal;
	word-break: break-word;
	font-size: 12px;
}

.tooltip .tooltiptext::after {
	content: "";
	position: absolute;
	bottom: 100%;
	left: 10%;
	margin-left: -5px;
	border-width: 10px;
	border-style: solid;
	border-color: transparent transparent #00A59B transparent;
}

.tooltip:hover .tooltiptext { visibility: visible; }
/* Tooltip Ends */

@media (min-width: 481px) and (max-width: 767px) {
    .form-steps{
        max-width: calc(100%);
	}
