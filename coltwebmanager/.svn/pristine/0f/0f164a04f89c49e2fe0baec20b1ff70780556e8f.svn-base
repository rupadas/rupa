/* Tooltip Starts */
.tooltip {
	position: relative;
	display: inline-block;
	opacity: 1;
  }
.tooltip-title {
    font-weight: bold;
}
  
  .tooltip .tooltiptext {
	visibility: hidden;
    width: 350px;
    background-color: #00A59B;
    color: #fff;
    border-radius: 2px;
    padding: 25px 0 25px 20px;
    position: absolute;
    z-index: 1;
    top: 130%;
    left: 90%;
    margin-left: -60px;
  }
  
  .tooltip .tooltiptext::after {
	content: "";
    position: absolute;
    bottom: 100%;
    left: 10%;
    margin-left: -5px;
    border-width: 10px;
    border-style: solid;
    border-color: transparent transparent #00A59B transparent;
  }
  
  .tooltip:hover .tooltiptext {
	visibility: visible;
  }
/* Tooltip Ends */

