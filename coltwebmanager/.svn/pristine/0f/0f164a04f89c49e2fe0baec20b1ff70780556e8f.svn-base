/* Steps Style Start */
.form-steps {
	display: block;
	width: 100%;
	position: relative;
	margin: 5px 0;
}

.form-steps:after {
		content: "";
		display: table;
		clear: both;
}

.form-steps__item {
		padding: 0;
		position: relative;
		display: block;
		float: left;
		width: 25%;
		text-align: center;
}

.form-steps__item-content {
		display: inline-block;
}

.form-steps__item-icon {
		background: #ece9e9;
		color: lightslategray;
		display: block;
		border-radius: 100%;
		text-align: center;
		width: 40px;
		height: 40px;
		line-height: 40px;
		margin: 0 auto 10px auto;
		position: relative;
		font-size: 13px;
		font-weight: 700;
		z-index: 2;
		border: 1px solid;
}

.form-steps__item-text {
		font-size: 13px;
		color: lightslategray;
		font-weight: 500;
}

.form-steps__item-line {
		display: inline-block;
		height: 2px;
		width: 100%;
		background: lightgray;
		float: left;
		position: absolute;
		left: -50%;
		top: 20px;
		z-index: 1;
		/* border: 1px solid lightslategray; */
}

.form-steps__item-active .form-steps__item-icon {
		background: green;
		color: white;
		border-color: mediumseagreen;
}

.form-steps__item-active .form-steps__item-text {
		color: green;
		font-weight: 600;
}

.form-steps__item-active .form-steps__item-line,
.form-steps__item-completed .form-steps__item-line {
		background: green;
		border: 1px solid mediumseagreen;
}

.form-steps__item-completed .form-steps__item-text {
		color: #4f5e77;
}
/* Steps Style Ends */

/* Buttons Starts*/
.custom-row {
    display: flex;
    flex-wrap: wrap;
    margin-right: -10px;
    margin-left: -10px;
}
.white-box {
    padding: 15px 0 0 25px;
}
.white-box .box-title {
    margin-bottom: 10px;
}
.btn-wrapper {
    position: relative;
    padding: 2px 3px 2px 2px;
    margin: 1rem -15px 0;
    margin-left: auto;
    margin-right: auto;
}
.btn-outline-success.custom-btn {
	-moz-box-shadow: 0px 0px 0px 0px #3dc21b;
	-webkit-box-shadow: 0px 0px 0px 0px #3dc21b;
	box-shadow: 0px 0px 0px 0px #3dc21b;
	background-color: #fff;
	-moz-border-radius:1px;
	-webkit-border-radius:1px;
	border-radius: 0px;
	border:1px solid #77b2d9;
	display:inline-block;
	cursor:pointer;
	color: #000!important;
	font-family:Arial;
	font-size: 14px;
	padding: 10px 42px;
	text-decoration:none;
	margin-right: 10px;
}
.btn-outline-success.custom-btn.active:not(:disabled):not(.disabled).active {
	background-color: #00A59B;
	color: #fff !important;
}
.btn-outline-success.custom-btn:hover {
	background-color:#00A59B;
}
.btn-outline-success.custom-btn:active {
	position:relative;
	top:1px;
}
/* Buttons Ends */

/* Toggle Buttons Start */
.toggle-wrapper {
	display: flex;
	flex-direction: row;
	/* align-items: left; */
	justify-content: left;
	/* background-color: #CCCCCC; */
  }
  .toggle-wrapper span.switcher {
	position: relative;
	width: 200px;
	height: 50px;
	border-radius: 25px;
	margin: 5px 0 0px;
  }
  .toggle-wrapper span.switcher input {
	-webkit-appearance: none;
	   -moz-appearance: none;
			appearance: none;
	position: relative;
	width: 255px;
	height: 30px;
	border-radius: 25px;
	font-size: 12px;
	background-color: #1E1E1E;
	outline: none;
	font-weight: 200;
	font-family: 'Oswald', sans-serif;
  }
  .toggle-wrapper span.switcher input:before, .toggle-wrapper span.switcher input:after {
	z-index: 2;
	position: absolute;
	top: 50%;
	-webkit-transform: translateY(-50%);
			transform: translateY(-50%);
	color: #FFFFFF;
  }
  .toggle-wrapper span.switcher input:before {
	content: 'Search by Address';
	left: 10px;
  }
  .toggle-wrapper span.switcher input:after {
	content: 'Search by Local Area';
	right: 5px;
  }
  .toggle-wrapper span.switcher label {
	z-index: 1;
	position: absolute;
	top: 0px;
    bottom: 12px;
	border: 1px solid #ccc;
	border-radius: 20px;
  }
  .toggle-wrapper span.switcher.switcher-1 input {
	transition: .25s -.1s;
  }
  .toggle-wrapper span.switcher.switcher-1 input:checked {
	background-color: #FFFFFF;
	border: 1px solid #ccc;
  }
  .toggle-wrapper span.switcher.switcher-1 input:checked:before {
	color: #FFFFFF;
	transition: color .5s .2s;
  }
  .toggle-wrapper span.switcher.switcher-1 input:checked:after {
	color: #000;
	transition: color .5s;
  }
  .toggle-wrapper span.switcher.switcher-1 input:checked + label {
	left: 0px;
	right: 75px;
	background: #00A59B;
	transition: left .5s, right .4s .2s;
  }
  .toggle-wrapper span.switcher.switcher-1 input:not(:checked) {
	background: #00A59B;
	transition: background .5s -.1s;
	border: 1px solid #00A59B;
  }
  .toggle-wrapper span.switcher.switcher-1 input:not(:checked):before {
	color: #fff;
	transition: color .5s;
  }
  .toggle-wrapper span.switcher.switcher-1 input:not(:checked):after {
	color: #000;
	transition: color .5s .2s;
  }
  .toggle-wrapper span.switcher.switcher-1 input:not(:checked) + label {
	left: 125px;
	right: -60px;
	background: #FFFFFF;
	transition: left .4s .2s, right .5s, background .35s -.1s;
  }

  @-webkit-keyframes turn-on {
	0% {
	  left: 100%;
	}
	100% {
	  left: 0%;
	}
  }
  @keyframes turn-on {
	0% {
	  left: 100%;
	}
	100% {
	  left: 0%;
	}
  }

  @-webkit-keyframes turn-off {
	0% {
	  right: 100%;
	}
	100% {
	  right: 0%;
	}
  }
  @keyframes turn-off {
	0% {
	  right: 100%;
	}
	100% {
	  right: 0%;
	}
  }

  
/* Toggle Buttons Ends */
