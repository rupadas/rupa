a{ 
    color: #00a59b!important;
}
a:hover{ 
    color: #008c83!important;
}
h3{
    font-family: 'Gotham A','Gotham B',sans-serif;
    font-weight: bold!important;
    color: #333;
    font-size: 1.75rem!important;
}
.co-form .btn-primary{
    background: #00a59b; 
    border: 1px solid #008c83;
    color: #fff!important;
}
.co-form .btn-primary:hover, .co-form .btn-primary:visited{
    background: #008c83; border: 1px solid #008c83;
}
.co-profile-status span{ 
    font-weight: bold;
 }
.co-required{
    color: #ff0000;
}
.m-t25{ margin-top: 25px; }
.m-t15{ margin-top: 15px; }
.m-b25{ margin-bottom: 25px; }
.m-l15{ margin-left:15px; }
.m-l10{ margin-left:10px; }
.m-r15{ margin-right:15px; }
.m-b0{ margin-bottom: 0!important; }
.p-b125{ padding-bottom: 125px; }
.co-table{ border: 1px solid #ddd; }
.co-table thead{ background: #008c83; }
.co-table thead th{ 
    color: #fff;
    font-size: .75rem;
    text-align: center;
    padding:.5rem .25rem .75rem 0.5rem!important; }
.co-table th, .co-table td{ padding:.5rem!important; }
.co-table td { 
    font-size: .75rem;
    text-align: center; }
.co-table td a{ color: #008c83; }
.co-table td.active{ color:#2dc750; }
.co-table td.in-active{ color:#dc3545; }
.co-heading{ 
    border-bottom: 1px solid #ddd;
    padding-bottom: .35rem;
    margin-bottom: 1.5rem;
    font-size: 1.75rem!important;
