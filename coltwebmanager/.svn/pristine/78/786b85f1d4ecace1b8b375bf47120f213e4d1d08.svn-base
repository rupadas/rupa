    color: #fff;
 }
 .co-stepper{     
    margin: 1rem 0rem 3rem 10rem;
    position: relative;
}
.co-stepper ul{
    list-style-type: none;
}
.co-stepper ul li{
    display: inline-block;
    width: 32%;
}
.co-stepper ul li .circle{
    border: 1px solid #ccc;
    width: 50px;
    height: 50px;
    display: inline-block;
    background: #ddd;
    border-radius: 25px;
}
.co-stepper ul li .circle .circle-no{
    position: absolute;
    margin: 10px 0px 0px 18px;
    color: #999;
}
.co-stepper ul li.selected .circle .circle-no{    
    color: #fff;
}
.co-stepper ul li.selected .circle{    
    background: #4aae3c;
    border: 1px solid #349626;    
}
.co-stepper-line{ 
    height: 10px;
    background: #ddd;
    position: relative;
    top: 1.95rem;
    z-index: -1;
    left: 5rem;
    width: 60%;
 }
 .co-stepper ul li .name{    
    position: absolute;
    margin: 50px 0px 0px -100px;
    width: 150px;
    text-align: center;
    color: #ccc;   
}
.co-stepper ul li.selected .name{    
    color: #349626;
    
}
.co-stepper-line-reserved{ 
    background: #4aae3c;
    height: 10px;
    position: absolute;
    width: 300px;
    z-index: -1;
    margin: 21px 0px 0px 50px;
 }
 .co-stepper-line-reserved.activated{     
    margin-left:350px;
 }
 .co-no-type label.btn{
    border: 3px solid #00a59b;
    padding: 1rem;
    margin: .75rem .75rem 0 0;
    display: block;
    background: transparent;
    color: #999;
    border-radius: 10px!important;
 }
 .co-no-type label.btn:hover,.co-no-type label.btn.active{
    text-decoration: none;
    border: 3px solid #00a59b!important;
    background: #00a59b!important;
    color: #fff;
}
.co-record-info{ 
    margin-top: 20px;
    display: block;
    color: #999;
    font-weight: bold;
 }
 .navigation.co-select-no-pg{
     border: 1px solid red!important;
 }
.co-nr-box{
    width: 75%;
    margin: .75rem auto 0;
}
.co-nr-box label{
    display: block;
    text-align: left;
    color: #fff;
    font-size: .85rem;
    line-height: .5rem;
}
.co-nr-box .form-group{
    margin: 0;
}



 /* Footer */
 .co-footer-bg{ 
    background: #3C3C3C;
    border-top: 1px solid #cccccc;
    padding: 1rem 0;
 }
 .co-footer-nav{
     display: block;
 }
 .co-footer-nav .list-group-item{ 
    padding: 0;
    background: transparent;
    border: 0;
    padding-left: 1rem;
    display: inline-block;
 }
 .co-footer-nav .list-group-item a{ 
    color: #00a59b;
 }
 .co-footer-text{
     color:#C3C3C3;
 }
 /* Footer Ends */

