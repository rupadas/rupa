}
.co-dropdown-filter button{ 
    border: 1px solid #ced4da;
    width: 100%;
    text-align: left;
}
.co-dropdown-filter .dropdown-toggle::after{
    position: absolute;
    right: 10px;
    top: 18px;
}
.co-dropdown-filter .dropdown-menu{
    width: 100%;
}
.co-dropdown-filter .co-search{
    margin: 0 auto;
    width: 95%;
}
.dropdown:hover .dropdown-menu {
    display: block;    
}
.co-form label{ 
    margin-bottom: .125rem;
 }
 ul.co-pagination li a{     
    color: #00a59b!important;    
  }
 ul.co-pagination li.active a{     
    background-color: #00a59b!important;
    border-color: #008c83!important;
    color: #fff!important;
  }
  ul.co-pagination li.disabled a{
      color: #6c757d!important;
  }

/* Header */
.co-header-bg{ 
    background: #3a3f51; 
    border-top: 4px solid #00a59b;
    padding: 10px 0;
 }
 .co-header-nav{
    padding: 0;
 }
 .co-header-nav .navbar-nav li a{
    color: #bdc0c4;
 }
 .co-header-nav .navbar-nav li a:hover,.co-header-nav .navbar-nav li.active a{
    color: #ffffff;
 }
 .co-header-nav .navbar-nav .nav-link{
    color:#fff!important;
 }
 .co-header-nav .dropdown-menu {
    padding: 0;
 }
 .co-header-nav .dropdown-menu a{
    color:#333!important;
    border-bottom:1px solid #ddd;
    border-radius: 5px 5px 0 0;
 }

 /* Header Ends */

 /* Breadcrum */
 .co-breadcrum{
    background: #e9ecef;
}
.co-breadcrum .breadcrumb{
    margin-bottom: 0;
    padding-left: 0;
}
 /* Breadcrum Ends */

 /* Search */
 .co-search-wrapper{
     background: #00a59b;
     padding: .75rem 0;
 }
 .co-search-box{
    background: #008c83;    
    padding: .75rem;
    text-align: center;    
}
/* Search Ends */
/* Choose profile */
 .co-cp-overlay{ 
    background: rgba(0,0,0,.75);
    height: 123%;
    width: 100%;
    position: absolute;
    z-index: 1;
}
.co-cp-container{ 
    position: absolute;
    z-index: 10;
}
.co-cp-popup{ 
    background: #fff;
    width: 455px;
    margin: 7rem auto;
    border: 1px solid #ddd;
    border-radius: 10px;
    padding: 10px;
}
.co-cp-popup h4{ 
    border-bottom: 1px solid #ddd;
    padding-bottom: .5rem;
 }
.co-cp-popup label{ 
    margin-bottom: 0;
    font-weight: bold;
    color: #555;
 }

 /* Choose profile */

 /* Dashboard */
 .co-dash{ 
    border-color: #ccc; 
    margin-bottom: 30px;
    display: block;
  }
.co-dash .top,.co-dash .bottom{    
    padding: 2rem 1rem;
}
.co-dash .bottom{    
    background: rgba(0,0,0,.65);
    padding: 1rem;
    border: 1px solid #6c757d;
}
.co-dash-circle{     
    border: 1px solid #6c757d;
    border-radius: 4rem;
    width: 6.75rem;
    display: inline-block;
    padding: 1.5rem; 
    background: #f4f4f4;
}
.co-dash-circle .fa{
    font-size: 3.5rem;    
}
.co-dash .text-center{
    display: block;
}
.fa-arrow-circle-right{ 
    font-size: 2.25rem!important; 
}
.co-label{ 
    font-size: 1.25rem; 
 }
 .co-dash-circle .fa,.fa-arrow-circle-right,.co-label { 
     color:#6c757d;
  }

  .fa-arrow-circle-right,.co-label { 
    color:#eaece7;
 }
 a.co-dash:hover .co-dash-circle, a.co-dash:hover .card, a.co-dash.active .co-dash-circle, a.co-dash.active .card{
     border: 1px solid #00a59b;
 }
a.co-dash:hover .co-dash-circle .fa, a.co-dash.active .co-dash-circle .fa{
    color: #00a59b;
} 
a.co-dash:hover .co-dash-circle,a.co-dash.active .co-dash-circle{
    background: #fff;
}
a.co-dash:hover .bottom a,a.co-dash.active .bottom a{
    color: #fff;
}
a.co-dash:hover .bottom, a.co-dash.active .bottom{
    background: #00a59b;
    border: 1px solid #00a59b;
}
.co-dash .fa-plus{ 
    position: absolute;
    font-size: 1.5rem;
    margin: 0px 0px 0px -20px;
 }
 /* Dashboard Ends */

 /* Tab */
 .nav-tabs.co-tabs{
    border-bottom: 1px solid #dee2e6;
    margin-bottom: 15px;
}
.nav-tabs.co-tabs li a{ 
    color:#999;
    border: 0; 
}
.nav-tabs.co-tabs li a:hover,.nav-tabs.co-tabs li a.active{ 
    border: 0;    
    border-bottom: 3px solid #00a59b;
    color: #00a59b;
}
.nav-tabs.co-tabs li a .co-info{
    color: #999;
    font-size: .85rem;
}
.co-tab-count{ 
    background: #00a59b;
    color: #fff;
    border-radius: 10px;
    font-size: .65rem;
    padding: 0rem .25rem;
    position: relative;
    top: -7px;
 }
 /* Tab Ends */
.co-search{ 
    width: 75%;
    margin: 0 auto;
}
.co-search .co-search-select{    
    border-radius: 5px 0 0 5px;
    border: 1px solid #ddd;
    border-right: 0;
    color: #777;
}
.co-search .btn-outline-secondary{
    background: #fff;
    border: 1px solid #ddd;
}
.co-search .btn-outline-secondary .fa-search{
    color: #00a59b;
}
.co-search .btn-outline-secondary:hover,.co-search .btn-outline-secondary:active,.co-search .btn-outline-secondary:focus{
    background: #fff;
    color:#333;
}
.co-searh-address-btn{
    margin-top: 1.5rem;
}
.co-form-note{    
    margin: .5rem 0;    
}
.co-form-note p{ 
    color: #bbb;
    margin-bottom: 0;
    font-style: italic;
    font-size: .85rem;    
 } 
 .co-modal .modal-body .form-row .row{     
     width: 100%;
 }
 .co-modal .modal-body .form-row .row .form-control-plaintext{
     color: #777;
 }
.co-new-no .form-group {     
    margin-bottom: 0;
 }
