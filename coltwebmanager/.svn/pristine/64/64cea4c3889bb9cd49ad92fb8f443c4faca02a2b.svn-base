<div class="col-md-12">
  <table mat-table [dataSource]="dataSource" class="mat-elevation-z8">
    <ng-container *ngFor="let disCol of displayedColumns; let colIndex = index" matColumnDef={{disCol}}>
      <ng-container *ngIf="disCol != 'checked' ">
        <mat-header-cell *matHeaderCellDef>{{disCol}}</mat-header-cell>
        <mat-cell *matCellDef="let element"> {{element[disCol]}} </mat-cell>
      </ng-container>
      <ng-container *ngIf="disCol == 'checked' ">
        <mat-header-cell *matHeaderCellDef>
          <mat-checkbox #ref (change)="matCheckChange($event)"></mat-checkbox>
        </mat-header-cell>
        <mat-cell *matCellDef="let element">
          <mat-checkbox [(ngModel)]="element.checked"></mat-checkbox>
        </mat-cell>
      </ng-container>
    </ng-container>
    <mat-header-row *matHeaderRowDef="displayedColumns"></mat-header-row>
    <mat-row *matRowDef="let row; columns: displayedColumns;"></mat-row>
  </table>
  <div class="paginator-wrapper">
    <mat-paginator [length]="length" [pageSize]="pageSize" [pageSizeOptions]="pageSizeOptions"
      (page)="pageEvent = $event">
    </mat-paginator>
    <div class="export-wrapper">
      <a routerLink=""><img src="assets/images/export.svg" class="export-img"><span class="export-text">Export to
          Excel</span></a>
    </div>
  </div>
  <div class="btn-group next-btn-box">
    <button class="btn btn-light next-btn" [disabled]='buttonDisable'
      [ngClass]="!buttonDisable ? 'active' : ''">Next</button>
  </div>
  <div class="btn-group prev-btn-box">
    <button class="btn btn-light next-btn prev-btn">Previous</button>
  </div>

</div>